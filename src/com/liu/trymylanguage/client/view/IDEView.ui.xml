<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:image field="bgImage" src="./resource/image/img01.gif" />
	<ui:style>
		@sprite .background{
				background-repeat: repeat;
				background-color: #C9CFCD;
				gwt-image: 'bgImage';
				
				
				
				font-size: 11pt;
				color: #323232;
				line-height: 1.75em;
				width:100%;
				height: 100%;
		}
	</ui:style>
	<g:DockLayoutPanel styleName="{style.background}">
		<g:north size="30">
			<g:HorizontalPanel>
				<g:Button ui:field="runButton">Run</g:Button>
				<g:Button ui:field="newTabButton">New Tab</g:Button>
			</g:HorizontalPanel>
		</g:north>
		<g:south size="2">
			<g:VerticalPanel></g:VerticalPanel>
		</g:south>
		<g:center>
			<g:SplitLayoutPanel>
				<g:west size="300">
					<g:HTMLPanel ui:field="tutorialArea"></g:HTMLPanel>
					
				</g:west>
				<g:north size="500">
					
					<g:TabLayoutPanel ui:field="tabPanel" barHeight="20">
						
					</g:TabLayoutPanel>
				
				
				</g:north>
				<g:center>
					
					<g:HTMLPanel ui:field="consoleArea">
						
					
					</g:HTMLPanel>
				
				</g:center>
			</g:SplitLayoutPanel>
		
		</g:center>
	
	</g:DockLayoutPanel>
</ui:UiBinder> 