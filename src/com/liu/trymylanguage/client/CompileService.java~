package com.liu.trymylanguage.client;

import com.google.gwt.user.client.rpc.RemoteService;
import com.google.gwt.user.client.rpc.RemoteServiceRelativePath;
import java.io.IOException;

@RemoteServiceRelativePath("compile")
public interface CompileService extends RemoteService {

    Process compile(String source) throws IOException;
    String fileName(String pattern,String filename);
}